generator client {
  provider                    = "prisma-client-py"
  enable_experimental_decimal = "true"
  recursive_type_depth        = "-1"
}

datasource db {
  provider     = "sqlserver"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model MD_ResultBox {
  Id       Int     @id(map: "MD_ResultBox_pk") @default(autoincrement())
  ResultId String  @db.NVarChar(100)
  X        Decimal @db.Decimal(12, 4)
  Y        Decimal @db.Decimal(12, 4)
  Width    Decimal @db.Decimal(12, 4)
  Height   Decimal @db.Decimal(12, 4)
  Accuracy Decimal @db.Decimal(8, 4)
  Label    String  @db.NVarChar(500)
}

model MD_Results {
  Id                  String   @id(map: "MD_Results_pk") @db.NVarChar(100)
  CaptureTime         DateTime @db.DateTime
  ProcessingStartTime DateTime @db.DateTime
  ProcessingDuration  Int
  CameraId            String   @db.NVarChar(100)
  CreatedAt           DateTime @default(now(), map: "DF__MD_Result__Creat__37A5467C") @db.DateTime
}
